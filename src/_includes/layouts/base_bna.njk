{% extends "layouts/base.njk" %}

{% block header %}
  <div id="site">{{ coursename }}</div>
  <div id="coursenum">COMM {{ coursenum }} ({{ year }})</div>
{% endblock %}

{% block sitenav %}
  <ul>
    <li><a href="/bna/{{ year }}/">Syllabus</a></li>
    <li><a href="/bna/{{ year }}/classnotes/">Class notes</a></li>
  <!-- Add topics and assignments pages in 2022 -->
  {% if year >= 2022 %}
    <li><a href="/bna/{{ year }}/topics/">Topics</a></li>
    <li><a href="/bna/{{ year }}/assignments/">Assignments</a></li>
  {% endif %}
  </ul>
{% endblock %}
